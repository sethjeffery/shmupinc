{
  "name": "shmupinc",
  "description": "An incrementally vibe-coded incremental shmup game.",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "content:print": "tsx scripts/content-cli.ts print",
    "content:scaffold": "tsx scripts/content-cli.ts scaffold",
    "content:validate": "tsx scripts/content-cli.ts validate",
    "find-orphans": "knip --include files,dependencies,unlisted,unresolved,exports,duplicates --no-progress",
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "test": "tsc --noEmit && eslint .",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/d3-selection": "^3.0.11",
    "@types/d3-zoom": "^3.0.8",
    "@types/node": "^25.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-perfectionist": "^5.3.1",
    "jsonc-parser": "^3.3.1",
    "knip": "^5.84.1",
    "monaco-editor": "^0.53.0",
    "tsx": "^4.20.5",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.53.0",
    "vite": "^7.2.4"
  },
  "dependencies": {
    "@preact/signals": "^2.7.1",
    "cac": "^6.7.14",
    "clsx": "^2.1.1",
    "d3-selection": "^3.0.0",
    "d3-zoom": "^3.0.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "fast-glob": "^3.3.3",
    "json-schema-traverse": "^1.0.0",
    "json5": "^2.2.3",
    "ky": "^1.14.3",
    "lucide-preact": "^0.575.0",
    "phaser": "^3.90.0",
    "preact": "^10.28.3",
    "prettier": "^3.8.1",
    "zod": "^4.1.11"
  }
}
